<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane onMouseClicked="#fullCard" onMouseReleased="#fullCard" prefHeight="64.0" prefWidth="600.0" style="-fx-border-color: BLACK;" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Rain.Spells.SpellCard">
   <userData>
      <fx:reference source="controller" />
   </userData>
   <children>
      <Label fx:id="Level" layoutY="20.0" text="Above 9th level Transmutation Wizard spell">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="Name" prefHeight="27.0" prefWidth="430.0" text="FireBall">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="Comp" alignment="CENTER_RIGHT" layoutX="412.0" minHeight="27.0" minWidth="171.0" prefHeight="27.0" prefWidth="193.0" text="Components: V, S, M">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="CastTime" layoutX="435.0" layoutY="20.0" minHeight="20.0" minWidth="171.0" prefHeight="20.0" prefWidth="171.0" text="Bonus Action">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="prepare" layoutX="2.0" layoutY="36.0" mnemonicParsing="false" onAction="#prepareSpell" prefHeight="27.0" prefWidth="81.0" style="-fx-background-radius: 0;" text="Prepare" />
      <Button fx:id="delete" layoutX="549.0" layoutY="36.0" mnemonicParsing="false" onAction="#deleteSpell" prefHeight="27.0" prefWidth="57.0" style="-fx-background-radius: 0;" text="Delete" />
   </children>
</AnchorPane>
